{
  "Deadline": "10m",
  "Enable": [
    "errcheck",
    "gofmt",
    "goimports",
    "gotype",
    "ineffassign",
    "interfacer",
    "megacheck",
    "nakedret",
    "misspell",
    "structcheck",
    "unparam",
    "unconvert",
    "unused",
    "varcheck",
    "vet",
    "vetshadow"
  ],
  "EnableGC": true,
  "Linters": {
    "errcheck": {
      "Command": "errcheck -abspath {not_tests=-ignoretests} -ignore github.com/hashicorp/terraform/helper/schema:ForceNew|Set -ignore io:Close -ignore github.com/terraform-providers/terraform-provider-azurerm/azurerm/helpers/azure:FlattenAndSetLocation"
    },
    "nakedret": {
      "Command": "nakedret -l 1"
    }
  },
  "Sort": [
    "path",
    "line"
  ],
  "Vendor": true,
  "WarnUnmatchedDirective": false
}
